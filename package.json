{
  "name": "@ohbug/cli",
  "version": "1.0.0",
  "description": "Command line utility to work with Ohbug",
  "keywords": [
    "ohbug",
    "source-map",
    "source-maps",
    "cli"
  ],
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "homepage": "https://github.com/ohbug-org/ohbug-cli",
  "bugs": {
    "url": "https://github.com/ohbug-org/ohbug-cli/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ohbug-org/ohbug-cli"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "ohbug": "dist/bin/ohbug.js"
  },
  "main": "dist/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc & tsc --module es6 --outDir ./dist/esm",
    "dev": "tsc -w",
    "test": "jest --config ./jest.config.js",
    "codecov": "codecov",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "commander": "^6.0.0",
    "form-data": "^3.0.0",
    "glob": "^7.1.6",
    "ora": "^4.0.5",
    "prompts": "^2.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/glob": "^7.1.3",
    "@types/jest": "^26.0.7",
    "@types/multer": "^1.4.3",
    "@types/node": "^14.0.27",
    "@types/prompts": "^2.0.8",
    "@types/rimraf": "^3.0.0",
    "codecov": "^3.7.2",
    "cz-conventional-changelog": "3.2.0",
    "express": "^4.17.1",
    "jest": "^26.2.1",
    "multer": "^1.4.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
